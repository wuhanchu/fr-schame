{
    "name": "z_knwon_info_web",
    "version": "1.0.0",
    "private": true,
    "description": "An out-of-box UI solution for enterprise applications",
    "scripts": {
        "analyze": "cross-env ANALYZE=1 umi build",
        "build": "cross-env BASE_PATH=/z_know_info umi build",
        "build:dataknown": "cross-env BASE_PATH=/z_know_info umi build",
        "deploy": "cross-env ANT_DESIGN_PRO_ONLY_DO_NOT_USE_IN_YOUR_PRODUCTION=site npm run site && npm run gh-pages",
        "fetch:blocks": "pro fetch-blocks",
        "format-imports": "cross-env import-sort --write '**/*.{js,jsx,ts,tsx}'",
        "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
        "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
        "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
        "lint-staged": "lint-staged",
        "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
        "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
        "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
        "lint:prettier": "check-prettier lint",
        "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
        "prettier": "prettier -c --write \"src/**/*\"",
        "start": "umi dev",
        "start:dev:out": "cross-env AUTH_URL=http://server.aiknown.cn:36024  SERVER_URL=http://server.aiknown.cn:36029 BASE_PATH=/z_know_info umi dev",
        "start:dev": "cross-env AUTH_URL=http://server.aiknown.cn:36024  SERVER_URL=http://server.aiknown.cn:36017  umi dev",
        "start:dev:server": "cross-env AUTH_URL=http://server.aiknown.cn:36024   SERVER_URL=http://127.0.0.1:5001  umi dev",
        "start:dev:local": "cross-env AUTH_URL=http://server.aiknown.cn:36024  SERVER_URL=http://127.0.0.1:5001  umi dev",
        "start:test": "cross-env AUTH_URL=https://server.aiknown.cn:31018/api/user_auth  SERVER_URL=https://server.aiknown.cn:31018/api/z_know_info umi dev",
        "start:local": "cross-env UMI_ENV=local umi dev",
        "start:no-mock": "cross-env MOCK=none umi dev",
        "test": "umi test",
        "test:all": "node ./tests/run-tests.js",
        "test:component": "umi test ./src/components",
        "ui": "umi ui",
        "upload:dev": "npm run build && git add . && git commit -m 'upload:dev' && git pull && git push origin -u develop",
        "upload:master": "npm run build && git add . && git commit -m 'upload:master' && git pull && git push origin -u master"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint-staged"
        }
    },
    "lint-staged": {
        "**/*.less": "stylelint --syntax less",
        "**/*.{js,jsx,tsx,ts,less,md,json}": [
            "prettier --write",
            "git add"
        ],
        "**/*.{js,jsx}": "npm run lint-staged:js",
        "**/*.{js,ts,tsx}": "npm run lint-staged:js"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 10"
    ],
    "dependencies": {
        "@ant-design/compatible": "1.0.5",
        "@ant-design/icons": "4.2.2",
        "@ant-design/pro-layout": "6.5.8",
        "@ant-design/pro-table": "2.9.9",
        "@types/minio": "^7.0.7",
        "antd": "4.6.5",
        "axios": "0.20.0",
        "braft-editor": "^2.3.9",
        "classnames": "^2.2.6",
        "client-oauth2": "4.3.3",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jest": "^23.20.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-unicorn": "^21.0.0",
        "file-saver": "^2.0.2",
        "jszip": "^3.2.2",
        "keyboardjs": "^2.6.4",
        "lodash": "4.17.20",
        "minio": "^7.0.18",
        "moment": "2.29.1",
        "nzh": "^1.0.4",
        "omit.js": "2.0.2",
        "path-to-regexp": "6.2.0",
        "qiankun": "^2.2.1",
        "qs": "^6.9.0",
        "react": "^16.13.16",
        "react-ace": "^9.3.0",
        "react-dom": "^16.8.6",
        "react-helmet-async": "^1.0.7",
        "react-json-view": "^1.19.1",
        "react-sound": "^1.2.0",
        "react-to-print": "2.10.3",
        "redux": "^4.0.1",
        "seamless-immutable": "^7.1.4",
        "smart-arraytotree": "^1.1.0",
        "styled-components": "5.1.1",
        "umi": "3.2.24",
        "umi-request": "^1.0.8",
        "use-merge-value": "^1.0.1",
        "uuid": "8.3.1",
        "wavesurfer.js": "4.6.0",
        "xlsx": "0.16.6",
        "zip-js": "^0.0.2"
    },
    "devDependencies": {
        "@ant-design/pro-cli": "^1.0.18",
        "@types/classnames": "^2.2.7",
        "@types/express": "^4.17.0",
        "@types/history": "^4.7.2",
        "@types/jest": "^25.1.0",
        "@types/lodash": "^4.14.144",
        "@types/qs": "^6.5.3",
        "@types/react": "^16.9.17",
        "@types/react-dom": "^16.8.4",
        "@types/react-helmet": "^5.0.13",
        "@umijs/fabric": "2.0.5",
        "@umijs/plugin-blocks": "^2.0.5",
        "@umijs/plugin-qiankun": "^2.14.3",
        "@umijs/preset-ant-design-pro": "^1.2.0",
        "@umijs/preset-react": "^1.4.8",
        "@umijs/preset-ui": "^2.0.9",
        "babel-eslint": "^10.1.0",
        "carlo": "^0.9.46",
        "chalk": "^4.0.0",
        "cross-env": "^7.0.0",
        "cross-port-killer": "^1.1.1",
        "detect-installer": "^1.0.1",
        "enzyme": "^3.11.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "6.14.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jest": "^24.1.5",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-unicorn": "^28.0.2",
        "express": "^4.17.1",
        "gh-pages": "^2.0.1",
        "husky": "^4.0.7",
        "jsdom-global": "^3.0.2",
        "lint-staged": "10.2.4",
        "mockjs": "^1.0.1-beta3",
        "prettier": "^2.0.1",
        "pro-download": "1.0.1",
        "puppeteer-core": "^3.0.0",
        "stylelint": "^13.0.0"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "checkFiles": [
        "src/**/*.js*",
        "src/**/*.ts*",
        "src/**/*.less",
        "config/**/*.js*",
        "scripts/**/*.js"
    ]
}

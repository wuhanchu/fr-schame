{
    "name": "z_knwon_info_web",
    "version": "1.0.0",
    "private": true,
    "description": "An out-of-box UI solution for enterprise applications",
    "scripts": {
        "analyze": "cross-env ANALYZE=1 umi build",
        "build": "umi build",
        "build:dataknown": "cross-env BASE_PATH=/z_know_info/ umi build",
        "deploy": "cross-env ANT_DESIGN_PRO_ONLY_DO_NOT_USE_IN_YOUR_PRODUCTION=site npm run site && npm run gh-pages",
        "fetch:blocks": "pro fetch-blocks",
        "format-imports": "cross-env import-sort --write '**/*.{js,jsx,ts,tsx}'",
        "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
        "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
        "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
        "lint-staged": "lint-staged",
        "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
        "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
        "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
        "lint:prettier": "check-prettier lint",
        "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
        "prettier": "prettier -c --write \"src/**/*\"",
        "start": "cross-env AUTH_URL=http://127.0.0.1:5000  SERVER_URL=http://127.0.0.1:5001 umi dev",
        "start:dev": "cross-env AUTH_URL=http://server.aiknown.cn:32024  SERVER_URL=http://server.aiknown.cn:32029  umi dev",
        "start:test": "cross-env AUTH_URL=https://server.aiknown.cn:31018/api/user_auth  SERVER_URL=https://server.aiknown.cn:31018/api/z_know_info umi dev",
        "start:local": "cross-env UMI_ENV=local umi dev",
        "start:no-mock": "cross-env MOCK=none umi dev",
        "test": "umi test",
        "test:all": "node ./tests/run-tests.js",
        "test:component": "umi test ./src/components",
        "ui": "umi ui",
        "upload:dev": "npm run build && git add . && git commit -m 'upload:dev' && git pull && git push origin -u develop",
        "upload:master": "npm run build && git add . && git commit -m 'upload:master' && git pull && git push origin -u master"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint-staged"
        }
    },
    "lint-staged": {
        "**/*.less": "stylelint --syntax less",
        "**/*.{js,jsx,tsx,ts,less,md,json}": [
            "prettier --write",
            "git add"
        ],
        "**/*.{js,jsx}": "npm run lint-staged:js",
        "**/*.{js,ts,tsx}": "npm run lint-staged:js"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 10"
    ],
    "dependencies": {
        "@ant-design/pro-layout": "^4.5.16",
        "@antv/data-set": "^0.10.2",
        "@elastic/react-search-ui": "^1.3.2",
        "@elastic/search-ui-app-search-connector": "^1.3.2",
        "antd": "3.25",
        "async-validator": "^2.0.0",
        "axios": "^0.19.0",
        "bizcharts": "^3.5.3-beta.0",
        "bizcharts-plugin-slider": "^2.1.1-beta.1",
        "classnames": "^2.2.6",
        "client-oauth2": "^4.2.5",
        "dva": "^2.4.1",
        "file-saver": "^2.0.2",
        "lodash": "^4.17.11",
        "lodash-decorators": "^6.0.1",
        "lodash.isequal": "^4.5.0",
        "moment": "^2.24.0",
        "numeral": "^2.0.6",
        "nzh": "^1.0.4",
        "omit.js": "^1.0.2",
        "path-to-regexp": "^3.1.0",
        "qs": "^6.9.0",
        "query-string": "^6.11.1",
        "react": "^16.8.6",
        "react-copy-to-clipboard": "^5.0.1",
        "react-document-title": "^2.0.3",
        "react-dom": "^16.8.6",
        "react-fittext": "^1.0.0",
        "redux": "^4.0.1",
        "smart-arraytotree": "^1.1.0",
        "tmp": "^0.1.0",
        "umi": "^3.1.0",
        "umi-plugin-pro-block": "^2.0.3",
        "umi-plugin-react": "^1.15.7",
        "umi-request": "^1.2.7",
        "xlsx": "^0.14.4",
        "zip-js": "^0.0.2"
    },
    "devDependencies": {
        "@ant-design/colors": "^3.1.0",
        "@ant-design/pro-cli": "^1.0.13",
        "@types/classnames": "^2.2.7",
        "@types/express": "^4.17.0",
        "@types/history": "^4.7.2",
        "@types/jest": "^24.0.13",
        "@types/lodash": "^4.14.144",
        "@types/qs": "^6.5.3",
        "@types/react": "^16.8.19",
        "@types/react-document-title": "^2.0.3",
        "@types/react-dom": "^16.8.4",
        "@umijs/fabric": "^1.1.0",
        "chalk": "^2.4.2",
        "check-prettier": "^1.0.3",
        "cross-env": "^6.0.0",
        "cross-port-killer": "^1.1.1",
        "enzyme": "^3.9.0",
        "eslint": "5.16.0",
        "eslint-plugin-prettier": "^3.1.1",
        "express": "^4.17.1",
        "gh-pages": "^2.0.1",
        "husky": "^3.0.0",
        "import-sort-cli": "^6.0.0",
        "import-sort-parser-babylon": "^6.0.0",
        "import-sort-parser-typescript": "^6.0.0",
        "import-sort-style-module": "^6.0.0",
        "jest-puppeteer": "^4.2.0",
        "lint-staged": "^9.0.0",
        "mockjs": "^1.0.1-beta3",
        "node-fetch": "^2.6.0",
        "prettier": "^1.18.2",
        "pro-download": "1.0.1",
        "slash2": "^2.0.0",
        "stylelint": "^10.1.0",
        "stylelint-config-prettier": "^6.0.0",
        "stylelint-prettier": "^1.1.1",
        "tslint-config-prettier": "^1.18.0",
        "tslint-plugin-prettier": "^2.0.1",
        "umi-plugin-ga": "^1.1.3",
        "umi-plugin-pro": "^1.0.2",
        "umi-types": "^0.5.0",
        "webpack-theme-color-replacer": "^1.2.15"
    },
    "optionalDependencies": {
        "puppeteer": "^1.17.0"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "checkFiles": [
        "src/**/*.js*",
        "src/**/*.ts*",
        "src/**/*.less",
        "config/**/*.js*",
        "scripts/**/*.js"
    ]
}
